		"prod": "yarn build && yarn start"
	},
	"devDependencies": {
		"@types/busboy": "^1.5.3",
		"@types/node": "^24.0.0",
		"@types/range-parser": "^1.2.7",
		"nodemon": "^3.0.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"colorts": "^0.1.63",
		"hyper-express": "^6.14.3"
	}
}
