{
	"name": "hyperexpress-typescript-template",
	"version": "1.0.0",
	"main": "server.js",
	"repository": "git@github.com:ViniciosLugli/hyperexpress-typescript-template.git",
	"author": "ViniciosLugli <vinicioslugli@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
